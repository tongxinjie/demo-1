	<select id="selectDetailsList" parameterType="java.lang.String" resultMap="BaseResultMap">
	     
	    select distinct * from(
        select
        user.avatar, user.login_name, user.wechat_id
		from user, chatroomlist2
		where (user.wechat_id = chatroomlist2.tomsg and (chatroomlist2.frommsg = #{mywechatid,jdbcType=VARCHAR})) or
		(user.wechat_id = chatroomlist2.frommsg and (chatroomlist2.tomsg = #{mywechatid,jdbcType=VARCHAR}))
		order by chatroomlist2.time desc) as a
	</select>
	
	
	
	
	
	<select id="selectAlladdrByHost" parameterType="java.lang.String" resultMap="BaseResultMap">
		select
		user.*
		from addrlist, user
	  where (addrlist.host = #{host,jdbcType=VARCHAR})
	  and (addrlist.userid = user.wechat_id)
	  order by user.groups
	</select>
	
	<select id="selectAllgroupsByHost" parameterType="java.lang.String" resultMap="BaseResultMap">
		select distinct user.groups
		from addrlist, user
	  where (addrlist.host = #{host,jdbcType=VARCHAR})
	  and (addrlist.userid = user.wechat_id)
	  order by user.groups
	</select>
	
	
	<select id="selectDetailsMoments" parameterType="java.lang.String" resultMap="BaseResultMap">
		select
		user.avatar, user.login_name
		from user, momentslist
		where (user.wechat_id = momentslist.wechatid)
		and (momentslist.host = #{host,jdbcType=VARCHAR})
